<?xml version="1.0" encoding="utf-8"  ?>
<openerp>
 <data>
     <!-- Search VIEW TABLET PRODUCTION -->
        <record model="ir.ui.view" id="view_prakruti_tablet_production_search">
            <field name="name">prakruti.tablet_production.search</field>
            <field name="model">prakruti.tablet_production</field>
            <field name="arch" type="xml">
                <search string="Product Wise Search">
                    <field name ="subplant_id"/>
                    <field name ="batch_id"/>
                    <field name ="batch_size" />
                    <field name ='status'/>
                </search>
            </field>
        </record>
      <!-- FORM VIEW TABLET PRODUCTION -->
     <record model="ir.ui.view" id="view_prakruti_tablet_production_form">
        <field name="name"> Tablet Production</field>
        <field name="model">prakruti.tablet_production</field>
        <field name="arch" type="xml">
            <form string="Tablet Production" >
                <header>
                    <field name='status' widget='statusbar'/>
                    <field name ='revise_status' widget='statusbar' attrs="{'invisible':[('is_revise', '=', False)]}"/>
                </header>
                <button name="allocate_batch_no" string="Allocate the Batch" type="object"  attrs="{'invisible':[('status', '=', 'batch_alloted')]}" class="oe_highlight"/>
                <group colspan="4" col="4">
                    <field name ="subplant_id" options="{'no_create': True, 'no_open': True, 'no_create_edit' : True}" domain="['&amp;',('plant_type','=','tablet'),('plant_allocated_flag','=',0)]" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="batch_id"  on_change="onchange_batch_id(batch_id)" domain="['&amp;',('batch_allocated_flag','=',0),('subplant_id','=',subplant_id)]" options="{'no_create': True, 'no_open': True, 'no_create_edit' : True}" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="batch_size" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="date" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="batch_allocation_date" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="batch_end_date" attrs="{'readonly':[('status', 'in', ('batch_alloted')),('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <field name ="created_by" options="{'no_create': True, 'no_open': True, 'no_create_edit' : True}"/>
                    <field name ="company_id" options="{'no_create': True, 'no_open': True, 'no_create_edit' : True}" />
                    <field name ="total_output_yeild" invisible="1"/>
                    <field name ="total_assay_output" invisible="1"/>
                    <field name ="available_qty" invisible="1"/>
                    <field name ="sieving"  invisible="True"/>
                    <field name ="preparation_of_binder"  invisible="True"/>
                    <field name ="granulation"  invisible="True"/>
                    <field name ="semi_drying"  invisible="True"/>
                    <field name ="final_drying"  invisible="True"/>
                    <field name ="milling"  invisible="True"/>
                    <field name ="api_addition" invisible="True"/>
                    <field name ="blending"  invisible="True"/>
                    <field name ="compression"  invisible="True"/>
                    <field name ="tablet_coating"  invisible="True"/>
                    <field name ="coating" invisible="True"/>
                    <field name ="inspection"  invisible="True"/>
                    <field name ="metal_inspection"  invisible="True"/>
                    <field name ="packing" invisible="True"/>
                    <field name ='production_name' invisible="True"/>
                    <field name ="bmr_no" invisible="True"/>
                    <field name ="flag_display_product" invisible="True"/>                             
                    <field name ="flag_delete_product" invisible="True"/>
                </group>
                
                <separator string ='Sieving' attrs="{'invisible':[('sieving', '=', 'no')]}"/>
                <field name='tablet_sieving_id' attrs="{'invisible':[('sieving', '=', 'no')]}"/>
                    <group class="oe_subtotal_footer oe_right">
                        <field name ="total_weight_before_si" attrs="{'invisible':[('sieving', '=', 'no')]}"/>
                        <field name ="total_weight_after_si" attrs="{'invisible':[('sieving', '=', 'no')]}" />
                    </group>
                
                <separator string ='Preparartion of Binder' attrs="{'invisible':[('preparation_of_binder', '=', 'no')]}"/>
                <field name='tablet_binder_id' attrs="{'invisible':[('preparation_of_binder', '=', 'no')]}"/>
                
                <separator string ='Granulation' attrs="{'invisible':[('granulation', '=', 'no')]}"/>
                <field name='tablet_granulation_id' attrs="{'invisible':[('granulation', '=', 'no')]}"/>
                
                <separator string ='Semi Drying' attrs="{'invisible':[('semi_drying', '=', 'no')]}"/>
                <field name='tablet_semi_drying_id' attrs="{'invisible':[('semi_drying', '=', 'no')]}"/>
                
                <separator string =' Final Drying'  attrs="{'invisible':[('final_drying', '=', 'no')]}"/>
                <field name='tablet_final_drying_id'  attrs="{'invisible':[('final_drying', '=', 'no')]}"/>
                
                <separator string ='Milling' attrs="{'invisible':[('milling', '=', 'no')]}"/>
                <field name='tablet_milling_id' attrs="{'invisible':[('milling', '=', 'no')]}"/>
                    <group class="oe_subtotal_footer oe_right">
                        <field name ="total_weight_before_mi" attrs="{'invisible':[('milling', '=', 'no')]}"/>
                        <field name ="total_weight_after_mi" attrs="{'invisible':[('milling', '=', 'no')]}" />
                    </group>
                    
                <separator string ='Other Ingredient/API addition' attrs="{'invisible':[('api_addition', '=', 'no')]}"/>
                <field name='tablet_api_addition_id' attrs="{'invisible':[('api_addition', '=', 'no')]}"/>
                
                <separator string ='Blending' attrs="{'invisible':[('blending', '=', 'no')]}"/>
                <field name='tablet_blending_id' attrs="{'invisible':[('blending', '=', 'no')]}"/>
                
                <separator string ='Compression\Capsule Filling' attrs="{'invisible':[('compression', '=', 'no')]}"/>
                <field name='tablet_compression_id' attrs="{'invisible':[('compression', '=', 'no')]}"/>
                
                <separator string ='Preparation Of Coating Solution ' attrs="{'invisible':[('tablet_coating', '=', 'no')]}"/>
                <field name='tablet_coating_preparation_id' attrs="{'invisible':[('tablet_coating', '=', 'no')]}"/>
                  <group class="oe_subtotal_footer oe_right">
                        <field name ="total_input_qty_pc" attrs="{'invisible':[('tablet_coating', '=', 'no')]}"/>
                        <field name ="total_output_qty_pc" attrs="{'invisible':[('tablet_coating', '=', 'no')]}"/>
                </group>
                
                <separator string ='Coating' attrs="{'invisible':[('coating', '=', 'no')]}"/>
                <field name='tablet_coating_id' attrs="{'invisible':[('coating', '=', 'no')]}"/>
                  <group class="oe_subtotal_footer oe_right">
                        <field name ="total_input_qty_c" attrs="{'invisible':[('coating', '=', 'no')]}"/>
                        <field name ="total_output_qty_c" attrs="{'invisible':[('coating', '=', 'no')]}"/>
                 </group>
                
                <separator string ='Tablet Inspection' attrs="{'invisible':[('inspection', '=', 'no')]}"/>
                <field name='tablet_inspection_id' attrs="{'invisible':[('inspection', '=', 'no')]}"/>
                
                <separator string ='Metal Detection' attrs="{'invisible':[('metal_inspection', '=', 'no')]}"/>
                <field name='tablet_metal_inspection_id' attrs="{'invisible':[('metal_inspection', '=', 'no')]}"/>

                <separator string ='Packing' attrs="{'invisible':[('packing', '=', 'no')]}"/>
                <field name='tablet_packing_id' attrs="{'invisible':[('packing', '=', 'no')]}"/>
                
                <separator string ='Break Down'/>
                <field name='breakdown_production_id'/>
                
                <separator string ='Remarks'/>
                <field name='remarks'/>
                    
                    
                <button name="action_list_products"  string="List" type="object" attrs="{'invisible':[('flag_display_product','=',1),('flag_delete_product','=',0)]}"/>
                <button name="action_delete_products"  string="Delete" type="object" attrs="{'invisible':[('flag_delete_product','=',1),('flag_display_product','=',0)]}"/>
                <separator string ='Output Details'/>
                    <field name='assay_line' />
                    
                <group colspan="4" col="4">
                    <button name="revise_tablet" string="Is Revision Required ?" type="object" class="oe_highlight" attrs="{'invisible':['|',('status', '!=', 'batch_alloted'),('revise_status', 'in', ('revise_tablet'))]}"/>                                         
                    <button name="revise_done" string="Revise Done" type="object" class="oe_highlight" attrs="{'invisible':[('revise_status', 'not in', ('revise_tablet'))]}"/>
                    <group colspan="4" col="4">
                        <field name ="revise_remarks" placeholder='Enter Revise Remarks Here...' attrs="{'invisible': [('is_revise', '=', False)]}"/>
                        <field name ="revise_id" options="{'no_create': True, 'no_open': True, 'no_create_edit' : True}" attrs="{'invisible': [('is_revise', '=', False)]}"/>
                        <field name ="is_revise" attrs="{'invisible':[('is_revise', '=', False)]}"/>
                        <field name ="revise_no" attrs="{'invisible':[('is_revise', '=', False)]}"/>
                        <field name ="revise_remarks_update" />
                        <field name ="revise_flag" invisible="True"/>
                    </group>
                </group>
            </form>
        </field>
    </record>
    
    
