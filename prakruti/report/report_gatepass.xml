<?xml version="1.0" encoding="utf-8"?>

<openerp>
    <data>
        <template id="report_gate_pass_document">
            <t t-name="report.external_layout_header">
                <div class="header" style=" width: 235mm; border:1px solid black; ">
                    <div class="row">
                        <div align="center" style="font-size:20px;"> <strong>GATE PASS</strong> </div>
                        <div class="col-xs-2">
                            <img src='/prakruti/static/src/img/logo.png' style="margin-left:3px;"/>
                        </div>
                        <div class="col-xs-10 text-right" style="margin-top:0px;margin-right:0px;">
                            <span t-field="o.company_id.rml_header1" /><br />
                            <address>
                                <strong> Address: </strong>
                                <span class="inline" t-field="o.company_id.name"/><br/>
                                <span class="inline" t-field="o.company_id.street"/> 
                                <span class="inline" t-field="o.company_id.street2"/> <br/>
                                <span class="inline" t-field="o.company_id.city"/>,
                                <span class="inline" t-field="o.company_id.zip"/>
                                <span class="inline" t-field="o.company_id.state_id"/><br/>
                                <strong> Phone No:</strong>
                                <span class="inline" t-field="o.company_id.phone"/>
                            </address>
                        </div>
                    </div>
                </div>
                </t>
            <t t-name="prakruti.report_gate_pass_document">
              <div class="page" style="width: 235mm; border:1px solid black; ">
                <div class="row" style="font-size:11px">
                    <div class="col-xs-12">
                        <table style="width: 235mm; border:1px solid black;">
                            <tr>
                                <td style="margin-top:1px;margin-left:1px;margin-right:1px;" ><b>GP No: </b> <span class="inline" t-field="o.pass_no"/></td>
                            </tr>
                            <tr>
                                <td style="margin-top:1px;margin-left:1px;margin-right:1px;"><b>Date:  </b> <span class="inline" t-field="o.pass_date"/></td>
                            </tr>
                            <tr>
                                <td style="margin-top:1px;margin-left:1px;margin-right:1px;font-family:italic"><p><strong>Following Materials are allowed to pass the premises</strong></p></td>
                            </tr>   
                            <tr>
                                <td style="margin-top:1px;margin-left:1px;margin-right:1px;"><b>Name of the party:</b><span class="inline" t-field="o.vendor_id"/> </td>
                            </tr>          
                        </table>
                        <br/>
                   
                        <th class="text-center" style="border: 1px solid #000000;"><strong>Inward/Outward Document Type</strong></th>
                        <td  style="border: 1px solid #000000;"><span t-field="o.document_type"/></td>
                        
                        
                        <br/>
                        <table  border="1" style="width: 235mm; border:1px solid black;">
                            <thead>
                                <tr>
                                    <th class="text-center" style="border: 1px solid #000000;"><strong>Sl No</strong></th>
                                    <th class="text-center" style="border: 1px solid #000000;"><strong>Name of the Product</strong></th>
                                    <th class="text-center" style="border: 1px solid #000000;"><strong>Qty as per Documents</strong></th>
                                    <th class="text-center" style="border: 1px solid #000000;"><strong>No of Packings</strong></th>
                                </tr>
                            </thead>
                            
                                <tr t-foreach="o.grid_id" t-as="line">
                                    <td class="col-xs-1" style="border: 1px solid #000000;">
                                        <span t-esc="line_index+1"/>
                                    </td>
                                    <td  style="border: 1px solid #000000;">
                                        <span t-field="line.product_id"/>
                                    </td>
                                    <td class="text-right"  style="border: 1px solid #000000;">
                                        <span t-field="line.quantity"/>
                                    </td>
                                    <td class="text-right"  style="border: 1px solid #000000;">
                                        <span t-field="line.no_of_packings"/>
                                    </td>
                                </tr>
                        </table>
                        <table border="1" style="width: 235mm; border:1px solid white;">
                            <tr>
                                <td>.</td>
                            </tr>
                            <tr>
                                <td align="left"><strong>Time of Movement: </strong><br></br><span t-field="o.time"/></td>
                                <td align="center"><strong>Plant Incharge: </strong><br></br><span t-field="o.plant_incharge"/></td>
                                <td align="right"><strong>Security Incharge:</strong><br></br><span t-field="o.security_id"/></td>
                            </tr>
                            <tr>
                                <td align="left"><strong>Vehicle No:</strong><span t-field="o.vehicle_no"/></td>
                                <td></td>
                                <td align="right"><p>Recieved above materials &amp; documents in good condition</p></td>
                        </tr>
                        <tr>
                            <td style="margin-top:1px;margin-left:1px;margin-right:1px;"><strong>Name of the Transporter: </strong><span t-field="o.transport_name"/></td>
                        </tr>
                        <tr>
                            <td style="margin-top:1px;margin-left:1px;margin-right:1px;"><strong>Name of the Driver: </strong><span t-field="o.driver_name"/></td>
                        </tr>
                        <tr>
                            <td>.</td>
                        </tr>
                        <tr>
                            <td style="margin-top:1px;margin-left:1px;margin-right:1px;"><strong>Security Register Entry No: </strong><span t-field="o.entry_no"/></td>
                            <td></td>
                            <td align="right"><strong>Signature</strong></td>
                        </tr>
                   </table>   
                 
                   <table border="1" style="width: 235mm; border:1px solid black;">
                        <tr>
                            <td align="left">Doc No.<span class="inline" t-field="o.document_no"/></td>
                            <td align="center">Rev. No.:<span class="inline" t-field="o.rev_no"/></td>
                            <td align="right">Date:<span class="inline" t-field="o.date"/></td>
                        </tr>
                    </table>
                  
                </div>
            </div>
        </div>   
     </t>
            <t t-name="report.external_layout_footer">
                <div class="footer">
                    <div class="text-center" style=" width: 235mm;">
                        <ul class="list-inline">
                            <li><strong> SUBJECT TO KARWAR JURISDICTION</strong> </li>
                        </ul>
                    </div>
                </div>
            </t>
        </template>           




